      "command": [
        "codex",
        "exec",
        "-c",
        "model_reasoning_effort=\"high\"",
        "--model",
        "{model}",
        "--json"
      ],
      "default_model": "gpt-5.2",
      "disabled": false,
      "kind": "codex_cli_jsonl",
      "modes": [
        "planner",
        "vision",
        "verifier",
        "chatter"
      ],
      "requires_auth": true,
      "roles": [
        "brain",
        "council"
      ],
      "static_models": [
        {
          "id": "gpt-5.2",
          "modalities": [
            "text",
            "vision"
          ],
          "notes": "Modelo premium (Codex CLI)"
